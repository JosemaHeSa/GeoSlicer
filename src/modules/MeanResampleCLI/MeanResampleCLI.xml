<?xml version="1.0" encoding="utf-8"?>
<executable>
  <category>LTrace Tools</category>
  <index>3</index>
  <title>MeanResample CLI</title>
  <description><![CDATA[LTrace Algorithms Wrapper]]></description>
  <version>0.0.1</version>
  <documentation-url>https://github.com/lassoan/SlicerPythonCLIExample</documentation-url>
  <license/>
  <contributor>LTrace Geophysics Team</contributor>
  <acknowledgements><![CDATA[LTrace Geophysical Solutions in partnership with Petrobras]]></acknowledgements>

  <parameters>
    <image>
      <name>inputVolume</name>
      <label>Input Volume</label>
      <longflag>input_volume</longflag>
      <description><![CDATA[Input volume to be resampled]]></description>
      <channel>input</channel>
      <default></default>
    </image>
    <image>
      <name>outputVolume</name>
      <label>Output Volume</label>
      <longflag>output_volume</longflag>
      <description><![CDATA[Resampled volume]]></description>
      <channel>output</channel>
      <default></default>
    </image>
    <string>
      <name>outputImageSpacing</name>
      <label>Output Spacing</label>
      <longflag>output_spacing</longflag>
      <description><![CDATA[Spacing along each dimension (0 means use input spacing)]]></description>
      <default>0,0,0</default>
    </string>
    <string-enumeration>
      <name>volumeType</name>
      <label>Volume Type (Scalar or Vector)</label>
      <longflag>volume_type</longflag>
      <description><![CDATA[Whether volume type is Scalar or Vector]]></description>
      <element>scalar</element>
      <element>vector</element>
      <default>scalar</default>
    </string-enumeration>
  </parameters>

</executable>
